const log = require('logToConsole');
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const aliasInWindow = require('aliasInWindow');
const callInWindow = require('callInWindow');
const setInWindow = require('setInWindow');
const callLater = require('callLater');
const createQueue = require('createQueue');

const ANALYTICS_URL = "https://cdn.dreamdata.cloud/scripts/analytics/v1/dreamdata.min.js";
const FORM_IDENTIFY_URL = 'https://cdn.dreamdata.cloud/scripts/identify-form/v1/identify-form.min.js';

const analyticsQueuePush = createQueue('analytics_queue');

const analytics_methods = [
  'identify',
  'track',
  'trackLink',
  'trackForm',
  'trackClick',
  'trackSubmit',
  'page',
  'pageview',
  'ab',
  'alias',
  'ready',
  'group',
  'on',
  'once',
  'off'
];

const method_factory = function(method) {
  return function() {
      const args = arguments.slice();
      args.unshift(method);
      analyticsQueuePush(args);
      return copyFromWindow('analytics_queue');
  };
};

const analytics = {};
for (let i = 0; i < analytics_methods.length; i++) {
  let method = analytics_methods[i];
  analytics[method] = method_factory(method);
}
setInWindow("analytics", analytics);

const initAnalytics = () => {
  const waitForAnalytics = () => {
    const initialize = copyFromWindow('analytics.initialize');
    if (initialize) {
      callInWindow('analytics.initialize', {
        "Dreamdata.io": {apiKey: data.apiKey}
      });

      copyFromWindow('analytics_queue').forEach(item => {
        const method = item.shift();
        const methodPath = 'analytics.' + method;
        if (copyFromWindow(methodPath)) {
          callInWindow(methodPath, item);
        }
      });

      if (data.autoIdentify) {
        injectScript(FORM_IDENTIFY_URL, data.gtmOnSuccess, data.gtmOnFailure, 'dreamdata-form-identify');
      } else {
        data.gtmOnSuccess();
      }
      
      callInWindow("analytics.page");
    } else {
      waitForAnalytics();
    }
  };
  callLater(waitForAnalytics);
};

injectScript(ANALYTICS_URL, initAnalytics, data.gtmOnFailure, 'dreamdata-analytics');


